{
  "comments": {
    "blockComment": [
      "[",
      "]"
    ],
  },
  "brackets": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ],
    [
      "(+",
      "+)"
    ],
    [
      "(-",
      "-)"
    ]
  ],
  "autoClosingPairs": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ],
    [
      "(+",
      "+)"
    ],
    [
      "(-",
      "-)"
    ],
    [
      "\"",
      "\""
    ]
  ],
  "surroundingPairs": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ],
    [
      "\"",
      "\""
    ]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^.*:$",
    "decreaseIndentPattern": "^\\s*[^:].*$"
  },
  "onEnterRules": [
    {
      "beforeText": "^\\s*$",
      "action": { "indent": "outdent" }
    }
  ]
}
